{
    "Name": "Microsoft Remote Help",
    "Source": "https://learn.microsoft.com/en-us/mem/intune/fundamentals/remote-help",
    "Get": {
        "Download": {
            "Uri": {
                "Production": "https://aka.ms/downloadremotehelp"
            },
            "MatchFwlink": "go.microsoft.com/fwlink/\\?linkid",
            "MatchFile": ".*msi$|.*exe$|.*zip$",
            "MatchVersion": "(\\d+\\.){3}\\d+",
            "DatePattern": "MM/dd/yyyy HH:mm:ss"
        }
    },
    "Install": {
        "Setup": "FSLogixAppsSetup.exe",
        "Preinstall": "Expand-Archive -Path #FileName -DestinationPath #TempPath",
        "Physical": {
            "Arguments": "/install /quiet /norestart",
            "PostInstall": []
        },
        "Virtual": {
            "Arguments": "/install /quiet /norestart",
            "PostInstall": []
        }
    }
}